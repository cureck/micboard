    "node": ">=20.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@ibm/plex": "^6.4.0",
    "@shopify/draggable": "^1.1.3",
    "babel-loader": "^9.1.3",
    "bootstrap": "^4.6.2",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "jquery": "^3.7.1",
