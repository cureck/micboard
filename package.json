{
  "name": "micboard",
  "version": "1.0.0",
  "description": "An intelligent mic tray for ethernet enabled Shure microphones.",
  "main": "main.js",
  "scripts": {
    "server": "python py/micboard.py",
    "binary": "pyinstaller py/micboard.spec",
    "build": "node node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
    "app": "electron .",
    "pack": "electron-builder --dir -c.mac.identity=null",
    "dist": "electron-builder"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karlcswanson/micboard.git"
  },
  "author": "karlcswanson@gmail.com",
  "bugs": {
    "url": "https://github.com/karlcswanson/micboard/issues"
  },
  "homepage": "https://github.com/karlcswanson/micboard",
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@ibm/plex": "^1.4.1",
    "@shopify/draggable": "^1.0.0-beta.8",
    "ajv": "^6.10.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^8.0.0",
    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "file-loader": "^3.0.1",
    "jquery": "^3.4.0",
    "node-sass": "^4.11.0",
    "npm": "^6.9.0",
    "popper.js": "^1.15.0",
    "qrcode": "^1.3.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0",
    "smoothie": "^1.35.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "electron": "^5.0.0",
    "electron-builder": "^20.39.0"
  },
  "build": {
    "appId": "com.micboard.app",
    "asar": true,
    "asarUnpack": [
      "dist/micboard-service",
      "build/trayTemplate.png",
      "build/trayTemplate@2x.png"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "extendInfo": {
        "LSBackgroundOnly": 1,
        "LSUIElement": 1
      }
    },
    "files": [
      "dist/micboard-service",
      "main.js",
      "build/trayTemplate.png",
      "build/trayTemplate@2x.png"
    ]
  }
}
